COUNT:
	PUSH 3
	CJNE R7,#0,JUG_Q4
	MOV 30H,R7
	MOV R3,30H
	LCALL PRINT
	POP 3
	RET
	
JUG_Q4:
	CJNE R7,#10,NEXT
NEXT:
	JNC Q3
Q4:
	MOV 30H,R7
	MOV R3,30H
	LCALL PRINT
	CLR P2.7
	POP 3
	RET
Q3:
	MOV A,R7
	MOV B,#10
	DIV AB
	MOV 30H,B
	MOV R3,30H
	LCALL PRINT
	CLR P2.7
	MOV R3,A
	LCALL PRINT
	CLR P2.6
	POP 3
	RET
	
	
	
	
	